{
  "name": "fish_api",
  "author": "ca",
  "version": "1.0.0",
  "description": "api for anglers",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "concurrently \"npm:build:watch\" \"nodemon\"",
    "build": "esbuild --platform=node src/index.ts --bundle --outfile=dist/index.js",
    "build:watch": "esbuild --platform=node src/index.ts --bundle --outfile=dist/index.js --watch",
    "typecheck": "tsc -noEmit",
    "clean": "rimraf dist"
  },
  "keywords": [],
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.19.2"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.8",
    "concurrently": "^8.2.2",
    "esbuild": "0.21.5",
    "nodemon": "^3.1.4",
    "prettier": "^3.3.2",
    "rimraf": "^5.0.7",
    "typescript": "^5.5.2"
  }
}
